<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="librarymanagmentsystem.ui.listbooks.ListBooksController">
    <stylesheets>
        <URL value="@listbooks.css" />
    </stylesheets>
   <children>
      <TableView fx:id="listbookstableview" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columns>
          <TableColumn fx:id="idcol" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="titlecol" prefWidth="75.0" text="Title" />
            <TableColumn fx:id="authorcol" prefWidth="75.0" text="Author" />
            <TableColumn fx:id="publishercol" prefWidth="75.0" text="Publisher" />
            <TableColumn fx:id="availablecol" prefWidth="75.0" text="Available" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
